{
  "manifest_version": 2,

  "name": "EdsDownload", 
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1.2",

  "browser_action": {
    },
  "background": {"persistent": false, "scripts": ["background.js"]}, 
   
  "permissions": [
    "activeTab", "downloads", "tabs",
    "https://ajax.googleapis.com/"
  ]
}

